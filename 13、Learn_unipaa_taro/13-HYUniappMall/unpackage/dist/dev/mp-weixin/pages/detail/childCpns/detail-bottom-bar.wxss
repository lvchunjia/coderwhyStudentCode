/* 水平间距 */
.detail-bottom-bar {
  height: 116rpx;
  position: fixed;
  background-color: #fff;
  left: 0;
  right: 0;
  bottom: 0;
  display: flex;
  text-align: center;
  border-top: 1rpx solid #f2f2f2;
}
.bar-item {
  flex: 1;
  display: flex;
}
.bar-item > .item {
  flex: 1;
}
.bar-left .text {
  font-size: 13px;
}
.bar-left .icon {
  display: block;
  width: 44rpx;
  height: 44rpx;
  margin: 20rpx auto 6rpx;
  background: url(data:image/png;base64,) 0 0/100%;
}
.bar-left .service {
  background-position: 0 -108rpx;
}
.bar-left .shop {
  background-position: 0 -196rpx;
}
.bar-right {
  font-size: 30rpx;
  color: #fff;
  line-height: 116rpx;
}
.bar-right .cart {
  background-color: #ffe817;
  color: #333;
  flex: 1;
}
.bar-right .buy {
  background-color: #f69;
  width: 186rpx;
}