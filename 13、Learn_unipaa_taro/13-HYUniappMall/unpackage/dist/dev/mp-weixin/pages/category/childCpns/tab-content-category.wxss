/* 水平间距 */
.tab-content-category {
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
}
.tab-content-category .content-item {
  display: flex;
  flex-direction: column;
  align-items: center;
  width: 33.33%;
}
.tab-content-category .content-item .c-img {
  width: 130rpx;
  height: 130rpx;
  margin-top: 30rpx;
}
.tab-content-category .content-item .title {
  margin-top: 30rpx;
  text-align: center;
  font-size: 24rpx;
  color: #666;
}